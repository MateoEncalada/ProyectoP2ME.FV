<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ProyectoP2ME.FV.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProyectoP2ME.FV"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Objetivos"
        ContentTemplate="{DataTemplate local:ObjetivosME_FV}"
        Route="Nuestros Objetivos" />
    <ShellContent
        Title="Recomendacion"
        ContentTemplate="{DataTemplate local:PrincipalME_FV}"
        Route="Profes" />
    
</Shell>
